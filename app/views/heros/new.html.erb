<div class="container">
  <div class="row">
    <div class="col">
    <h1>Create a new heroe</h1>
      <div>
          <%= simple_form_for @hero do |f|  %>
          <%= f.input :name  %>
          <%= f.input :power  %>
          <%= f.input :description  %>
          <%= f.input :photo, as: :file  %>
          <%= f.submit %>
        <% end %>
      </div>
    </div>
  </div>
</div>
